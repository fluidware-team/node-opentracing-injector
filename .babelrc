{
  "presets": [[
    "@babel/preset-env",
    {
      "targets": {
        "node": "current"
      }
    }
  ]],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "jsnext": {
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          { "useESModules": true, "version": "^7.12.1" }
        ]
      ]
    }
  }
}
